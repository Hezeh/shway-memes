{
  "short_name": "Memes",
  "name": "Shway Memes",
  "description": "A cool application that helps you share memes easily.",
  "dir": "auto",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    }
  ],
  "share_target": {
    "action": "/share_target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [{
        "name": "file",
        "accept": ["*/*"]
      }]
    }
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "categories": ["social", "entertainment", "photo", "lifestyle"]
}
